{
  "manifest_version": 2,
  "name": "Autofill4JobApply",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
